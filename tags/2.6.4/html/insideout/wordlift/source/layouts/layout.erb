<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= data.page.title || "The Middleman" %></title>

    <%= stylesheet_link_tag "wordlift.disambiguation", "wordlift.job", "wordlift.disambiguate" %>

    <%= javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js" %>
    <%= javascript_include_tag "http://code.angularjs.org/1.0.1/angular-1.0.1.min.js" %>
  </head>
  
  <body class="<%= page_classes %>">
    <input type='hidden' id='post_ID' name='post_ID' value='10780' />

    <%= yield %>

    <%= javascript_include_tag  "wordlift.disambiguation", "wordlift.job", "wordlift.disambiguate" %>
  </body>
</html>