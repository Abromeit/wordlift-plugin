{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "ajaxurl": true,
    "angular": true,
    "Backbone": true,
    "ENV": true,
    "inlineEditPost": true,
    "jQuery": true,
    "log": true,
    "tinyMCE": true,
    "wlSettings": true,
    "wp": true,
    "_": true
  },
  "extends": "wpcalypso/react",
  "rules": {
    "no-mixed-spaces-and-tabs": [
      "error",
      "smart-tabs"
    ],
    "wpcalypso/jsx-classname-namespace": "off"
  }
}