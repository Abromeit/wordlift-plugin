{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "ENV": true,
    "inlineEditPost": true,
    "wlSettings": true,
    "wp": true,
    "ajaxurl": true,
    "jQuery": true
  },
  "extends": "wpcalypso/react",
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": [
      "error",
      "tab"
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}